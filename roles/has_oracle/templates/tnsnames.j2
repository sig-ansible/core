{% for entry in tnsnames_entries %}
  {% set connect_data_extras = [ entry.connect_data_extras|default([]) ]|flatten %}
{{ entry.alias | default(entry.sid) }} =
  (DESCRIPTION =
    (ADDRESS = (PROTOCOL = TCP)(HOST = {{entry.host}})(PORT = {{ entry.port | default(1521) }}))
    (CONNECT_DATA =
      (SERVER = DEDICATED)
      {% if entry.service_name is defined %}
      (SERVICE_NAME = {{entry.service_name}})
      {% else %}
      (SID = {{ entry.connect_sid|default(entry.sid) }})
      {% endif %}
      {% for e in connect_data_extras %}
      {{ e }}
      {% endfor %}
    )
  )

{% endfor %}
